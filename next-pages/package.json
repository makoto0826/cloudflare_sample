{
  "name": "next-pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:dev": "wrangler pages dev .vercel/output/static --d1 --compatibility-flag=nodejs_compat",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
