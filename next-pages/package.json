  },
  "dependencies": {
    "@cloudflare/d1": "^1.4.1",
    "daisyui": "^2.51.5",
    "next": "^13.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^0.10.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "autoprefixer": "^10.4.14",
    "eslint": "7.32.0",
    "eslint-config-next": "^13.4.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "4.9.3",
    "wrangler": "^3.0.0"
  }
